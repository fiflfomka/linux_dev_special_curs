#: bin_searcher.c:21
#, c-format
msgid "Pick a number in range 1..%u (include 1 and %u).\n"
msgstr "Выберите число 1..%u (включая 1 и %u).\n"

#: bin_searcher.c:22 bin_searcher.c:45
#, c-format
msgid "Use only Yes/yes/Y/y and No/no/N/n to answer.\n"
msgstr "Для ответов используйте только Yes/yes/Y/y и No/no/N/n.\n"

#: bin_searcher.c:28
#, c-format
msgid "Is the number above than %u?\n"
msgstr "Ваше число строго больше чем %u?\n"

#: bin_searcher.c:34
#, c-format
msgid "Stopped. Your number is %d <= ? <= %d.\n"
msgstr "Вы завершили игру. Ваше число находится в промежутке: %d <= ? <= %d.\n"

#: bin_searcher.c:50
#, c-format
msgid "The number is %u!\n"
msgstr "Ваше число: %u!\n"
